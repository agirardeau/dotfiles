alias la='ls -A'
alias ll='ls -l'
alias lla='ls -lA'

alias tmux='tmux -2'

alias findr='find . -name'

alias grepr='grep -rniI . -e'
alias greprs='grep -rnI . -e'

alias psfind='ps -A | grep'

alias prettyjson='python -m json.tool'

